{
  "name": "dormjs",
  "version": "1.0.0",
  "description": "A light-weight ORM tool written with Typescript for Node.js based on Data Mapper pattern.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./dist/index.js",
    "prepublish": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akadirdev/dorm.git"
  },
  "keywords": [
    "orm",
    "postgresql",
    "rdbms",
    "nodejs",
    "typescript",
    "data mapper",
    "entity",
    "repository",
    "transaction",
    "database",
    "data"
  ],
  "author": "akadirdev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akadirdev/dorm/issues"
  },
  "homepage": "https://github.com/akadirdev/dorm#readme",
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "pg": "^8.7.3",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "pg": "^8.7.3"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    }
  }
}
